  "scripts": {
    "serve": "node ./dist/server.js",
    "build": "./node_modules/typescript/bin/tsc",
    "start:local": "NODE_ENV=local nodemon --delay 1500ms --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "start:development": "tsc && nodemon --exec NODE_ENV=development node ./dist/server.js",
    "start:production": "tsc && nodemon --exec NODE_ENV=production node ./dist/server.js",
    "prestart": "npm run build",
    "start": "node ./dist/server.js",
    "test": "NODE_ENV=local nodemon --watch 'src/**/*.ts' --exec 'ts-node node_modules/jasmine/bin/jasmine'",
    "debug": "tsc &&  debug node ./dist/server.js",
    "lint": "tslint -p tsconfig.json"
  },
